
        <div class="friends">
            <header>
                <nav>
                    <h3 class="logo">FRIENDS</h3>
                    <svg class= "menu" width="43" height="23" viewBox="0 0 43 23" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <line y1="1.5" x2="43" y2="1.5" stroke="white" stroke-width="3"/>
                        <line y1="11.5" x2="28" y2="11.5" stroke="white" stroke-width="3"/>
                        <line y1="21.5" x2="16" y2="21.5" stroke="white" stroke-width="3"/>
                    </svg>
                    
                    <div class="nav-open">
                        <div class="contact">
                            <h3><a href="/form" class="text-dark text-decoration-none">Become a Bajgain Now !</a></h3>
                        </div>
                        <div class="social">
                            <h3 style="padding-bottom: 25px;">Social</h3>
                            <div class="social-links">
                                <img src="public/Assets/Instagram.svg" alt="">
                                <img src="public/Assets/Twitter.svg" alt="">
                                <img src="public/Assets/Facebook.svg" alt="">
                            </div>
                            
                        </div>
                    </div>
                </nav>
                <main>
                        
                    <% friends.forEach((friend, i)=>{ %>
                        <section class="page <%= i === 0 ? 'photographer' : 'chef'%>">
                            <div class="details">
                                <h1><%= friend.fullname %></h1>
                                <h2><%= friend.title %></h2>
                                <p><%= friend.birthday.toUTCString().split(' ').splice(0,4).join(' ') %></p>
                            </div>
                            <div class="hero">
                                <img class="model-left"src=<%- `friends/${friend.id}/pictureleft`%> alt="split-left">
                                <img class= "model-right"src=<%- `friends/${friend.id}/pictureright`%> alt="split-right">
                            </div>
                        </section>
                    <% }) %>
                    
