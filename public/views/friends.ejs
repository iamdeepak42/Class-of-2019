                        <% for(let i = 0; i < friends.length; i++) { %>
                        <div class="page-<%= i %> <% if(i == 0){ %> active <%}%>">
                            
                            <svg class = "slide" width="12" height="12" viewBox="0 0 12 12" fill="none" xmlns="http://www.w3.org/2000/svg">
                                <circle cx="6" cy="6" r="6" fill="white" fill-opacity="1"/>
                            </svg>
